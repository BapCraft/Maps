<map>
    <name>Mario World</name>

    <description>Basic Team Deathmatch</description>

    <authors>
        <author>Etaishvetz</author>
    </authors>

    <difficulty>NORMAL</difficulty>

    <friendlyfire>off</friendlyfire>

    <modules>
        <module name="spawns"/>
        <module name="teams"/>
        <module name="autorespawn"/>
        <module name="gamerules"/>
        <module name="kits"/>
        <module name="pickkits"/>
        <module name="timelimit"/>
        <module name="building"/>
        <module name="removeitems"/>
        <module name="timelimit"/>
        <module name="building"/>
        <module name="autojoin"/>
        <module name="disabledamage"/>
        <module name="reports"/>
        <module name="nohunger"/>
        <module name="spectatoritems"/>
        <module name="mobspawning"/>
    </modules>

    <mobspawning>
        <disabled>
            <CREEPER/>
            <SKELETON/>
            <ZOMBIE/>
            <ENDERMAN/>
            <PIG/>
            <SLIME/>
        </disabled>
        <enabled></enabled>
    </mobspawning>

    <disabledamage>
        <type>FALL</type>
    </disabledamage>

    <building>
        <break>
            <enabled>
            </enabled>
            <disabled>
                <all/>
            </disabled>
        </break>

        <place>
            <enabled>
            </enabled>
            <disabled>
                <all/>
            </disabled>
        </place>
    </building>

    <timelimit updatetitle="true">
        <time>600</time>
    </timelimit>

    <spawns beds="off">
        <spawn team="blue">
            <loc x="10" y="99" z="-59"/>
        </spawn>

        <spawn team="red">
            <loc x="14" y="96" z="-2"/>
        </spawn>
        
        <spawn team="green">
            <loc x="42" y="93" z="34"/>
        </spawn>

        <spawn team="yellow">
            <loc x="26" y="87" z="-92"/>
        </spawn>

        <spectators>
            <loc x="13" y="64" z="-116"/>
        </spectators>
    </spawns>

    <teams>
        <team>
            <id>red</id>
            <color>RED</color>
            <mode>ADVENTURE</mode>
            <max>12</max>
            <min>3</min>

            <goals>
                <goal name="timerunout" id="time" permanent="false" showonscoreboard="false" visible="false"/>
                <goal name="killmostpeople" id="Kill the most people" permanent="false" showonscoreboard="false"
                      depends="time"/>
            </goals>
        </team>

